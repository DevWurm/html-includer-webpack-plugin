{
  "name": "include-loader",
  "version": "0.0.0",
  "description": "Webpack loader to include file contens into an HTML output via html-includer-webpack-plugin",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha",
    "clean": "rm -rf lib",
    "build": "npm run clean && babel src -d lib",
    "build-dev": "npm run clean && babel src -d lib --source-maps \"both\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevWurm/html-includer-webpack-plugin"
  },
  "keywords": [
    "Webpack",
    "Html"
  ],
  "author": "DevWurm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevWurm/html-includer-webpack-plugin/issues"
  },
  "homepage": "https://github.com/DevWurm/html-includer-webpack-plugin#readme",
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "6.13.2"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "cheerio": "0.20.0"
  }
}
